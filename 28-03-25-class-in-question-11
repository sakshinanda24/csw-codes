package lab_csw;
class MyText implements CharSequence{
	private String data;
	public MyText(String data) {this.data= data;}
	public int length() {return data.length();}
	public char charAt(int index) {return data.charAt(index);}
	public CharSequence subSequence(int start, int end) {return data.substring(start, end);}
	}
	
}
public class CharSequence_1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
CharSequence cs= new MyText("Java");
System.out.println(cs.length());
	}

}
