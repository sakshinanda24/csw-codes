package lab_csw;

public class Demo4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
 String s1= "Hello";
 String s2= "hello";
 System.out.println(s1.equalsIgnoreCase(s2));  //Case-Insensitive Comparison:
		
 String str= "Programming";
 System.out.println(str.indexOf('g'));
 System.out.println(str.lastIndexOf('g'));  //Find first and last occurrence of a character:
 
 String str1= "Java Programming";
 System.out.println(str1.startsWith("Java"));
 System.out.println(str1.endsWith("Python")); //Checking if a string starts or ends with a specific subString.
 
 String number= "123";
 int num= Integer.parseInt(number);
 System.out.println(num+10); //Convert String to Integer.
 
 int num1= 123;
 String str2= String.valueOf(num1);
 System.out.println(str2+ " is a number."); //Convert Integer to String.
 
 String str3= "Hello";
 char[]chars= str3.toCharArray();
 System.out.println(chars[1]); //Convert String to Character Array.

 
 //StringBuilder
 StringBuilder sb= new StringBuilder("Java");
 sb.append("Programming");
 System.out.println(sb); //output:Java Programming

 
	}

}

 
 
	


